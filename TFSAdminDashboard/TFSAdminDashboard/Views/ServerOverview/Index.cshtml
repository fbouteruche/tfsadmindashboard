
@model TFSAdminDashboard.Models.ServerOverviewModel

<div class="row">
    <div class="panel panel-primary">
        <div class="panel-heading">
            Server Overview - @model.Url
        </div>
        <div class="panel-body">
            <ul class="nav nav-tabs" role="tablist">
                <li class="active"><a href="#identityservice" role="tab" data-toggle="tab">Identity Service <img class="throb" id="identityThrobber" src="~/Img/ajax-loader.gif" /></a></li>
                <li><a href="#projectcollections" role="tab" data-toggle="tab">Project Collections <img class="throb" id="collectionThrobber" src="~/Img/ajax-loader.gif" /></a></li>
            </ul>
            <ul class="nav nav-tabs" role="tablist">
                
            </ul>
            <div class="tab-content">
                <div class="tab-pane active" id="identityservice" data-action="@Url.Action("IdentityOverview")"></div>
                <div class="tab-pane" id="projectcollections" data-action="@Url.Action("CollectionOverview")"></div>
            </div>
        </div>
    </div>
</div>

@section scripts{
    <script src="~/Scripts/ServerOverview.js"></script>
}
